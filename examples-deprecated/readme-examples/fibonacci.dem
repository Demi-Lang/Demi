fn fib(n) {
    let a = 0; let b = 1; let c = 0; let i = 0;
    if (n == 0) {
        return a;
    }

    # Replicate a for loop with a while loop
    i = 2;
    while (i <= n) {
        c = a + b;
        a = b
        b = c;
        i = i + 1;
    }
    return b;
}

print("First 10 Fibonacci numbers: ");
let i = 1;
while (i < 10) {
    print(fib(i));
    i = i + 1;
}

